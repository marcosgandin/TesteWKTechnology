unit Controller.Clientes;

interface

uses
  Windows, SysUtils, Classes, Controls, Forms, ComCtrls,
  Firedac.Stan.Param, FireDAC.Comp.Client, Data.DB, unit_funcoes,
  Model.Clientes;

type
  TControllerClientes = class
  public
    procedure Salvar(const objClientes: TClientes);
  end;

implementation

{ TControllerClientes }

procedure TControllerClientes.Salvar(const objClientes: TClientes);
begin
  objClientes.Salvar(objClientes);
end;

end.
